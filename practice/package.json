{
  "name": "bedrin-xsolla-challenge",
  "packageManager": "yarn@4.5.1",
  "author": "I.E. OLEG BEDRIN <simplavero@gmail.com>",
  "scripts": {
    "c": "npx hardhat compile",
    "cf": "npx hardhat compile --force",
    "t": "npx hardhat test",
    "tt": "npx hardhat test --trace",
    "tp": "npx hardhat test --parallel",
    "tsc": "npx tsc --noEmit",
    "bc": "npx hardhat node",
    "co": "npx hardhat coverage",
    "s": "npx solhint contracts/Test.sol",
    "su": "surya mdreport DogeGoGo.md contracts/DogeGoGo.sol",
    "so": "npx solium -f contracts/Test.sol",
    "ds": "npx hardhat run ./scripts/deploy.ts --network sepolia",
    "dl": "npx hardhat run ./scripts/deploy.ts --network localhost",
    "dlp": "npx hardhat run ./scripts/deployPrivate.ts --network localhost",
    "de": "npx hardhat run ./scripts/deploy.ts --network ethereum",
    "dep": "npx hardhat run ./scripts/deployPrivate.ts --network ethereum",
    "dm": "npx hardhat run ./scripts/deploy.ts --network mumbai",
    "dmp": "npx hardhat run ./scripts/deployPrivate.ts --network mumbai",
    "dbp": "npx hardhat run ./scripts/deployPrivate.ts --network besu",
    "dp": "npx hardhat run ./scripts/deploy.ts --network polygon",
    "dpp": "npx hardhat run ./scripts/deployPrivate.ts --network polygon",
    "v": "npx hardhat --network polygon verify 0x0 --contract contracts/Test.sol:Test",
    "n": "npx hardhat node"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-ignition": "^0.15.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "@nomicfoundation/ignition-core": "^0.15.5",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "4",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "chai": "4",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.4",
    "ethlint": "^1.2.5",
    "hardhat": "^2.22.15",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-docgen": "^1.3.0",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-typechain": "^0.3.5",
    "prettier": "^3.3.2",
    "solhint": "^5.0.1",
    "solidity-coverage": "^0.8.13",
    "solium": "^1.2.5",
    "surya": "^0.4.11",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "dlt-tools-solidity": "^1.0.10",
    "hardhat-tracer": "^3.1.0"
  }
}
